<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.collections.FXCollections?>

<BorderPane id="borderPane" fx:controller="sample.Controller"
            xmlns:fx="http://javafx.com/fxml">
    <top>
        <HBox alignment="BASELINE_LEFT" spacing="10">
            <Button fx:id="filKnapp" onAction="#velgFil" text="Velg labyrint fra fil"/>
            <ComboBox fx:id="utveivelger">
                <items>
                    <FXCollections fx:factory="observableArrayList">
                    </FXCollections>
                </items>
            </ComboBox>
        </HBox>
    </top>

    <center>
        <GridPane fx:id="grid" alignment="CENTER" gridLinesVisible="true">

        </GridPane>
    </center>
</BorderPane>

